  "Example FHIR Location PCA physical": {
    "isFileTemplate": true,
    "scope": "xml",
    "prefix": "Location-pca-physical",
    "body": [
      "<?xml version=\"1.0\" encoding=\"utf-8\"?>",
      "${BLOCK_COMMENT_START} TODO customise to your scenario / use case ${BLOCK_COMMENT_END}",
      "<Location xmlns=\"http://hl7.org/fhir\">",
      "  <id value=\"${TM_FILENAME/(.*)\\..+$/$1/}\"/>",
      "  <meta>",
      "    <profile value=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-location\"/>",
      "  </meta>",
      "  ${BLOCK_COMMENT_START} optional extension PCA Amenity ${BLOCK_COMMENT_END}",
      "  <extension url=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-amenity\">",
      "    <valueCodeableConcept>",
      "      <coding>",
      "        <system value=\"http://pca.digitalhealth.gov.au/fhir/4.0/CodeSystem/pca-amenity\"/>",
      "        <code value=\"${1|wheelchairAccess,hearingInductionLoop,ttyFacilities,brailleSignage,publicTelephone,parkingFree,parkingPaid,babyChangeFacility,childMindingOrCreche,accessibleFriendlyToilet,toilet,cafeteria,accessibleParking,accessibleTelephones,lowSensorySpace|}\"/>",
      "        <display value=\"TODO-display-value-for-${1}\"/>",
      "      </coding>",
      "    </valueCodeableConcept>",
      "  </extension>",
      "  ${BLOCK_COMMENT_START} optional extension PCA Postal Address ${BLOCK_COMMENT_END}",
      "  <extension url=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-postal-address\">",
      "    <valueAddress>",
      "      ${BLOCK_COMMENT_START} add optional GNAF Identifier extension ${BLOCK_COMMENT_END}"
      "      <extension url=\"http://hl7.org.au/fhir/StructureDefinition/address-identifier\">",
      "        <valueIdentifier>",
      "          <type>",
      "            <coding>",
      "              <system value=\"http://terminology.hl7.org.au/CodeSystem/v2-0203\"/>",
      "              <code value=\"GNAF\"/>",
      "              <display value=\"Geocoded National Address File Identifier\"/>",
      "            </coding>",
      "            <text value=\"G-NAF Identifier\"/>",
      "          </type>",
      "          <system value=\"http://hl7.org.au/id/gnafidentifier\"/>",
      "          <value value=\"GAVIC${RANDOM}${RANDOM/\\d{3}$//}\"/>",
      "        </valueIdentifier>",
      "      </extension>",
      "      ${BLOCK_COMMENT_START} add optional DPID Identifier extension ${BLOCK_COMMENT_END}"
      "      <extension url=\"http://hl7.org.au/fhir/StructureDefinition/address-identifier\">",
      "        <valueIdentifier>",
      "          <type>",
      "            <coding>",
      "              <system value=\"http://terminology.hl7.org.au/CodeSystem/v2-0203\"/>",
      "              <code value=\"DPID\"/>",
      "              <display value=\"Australia Post Delivery Point Identifier\"/>",
      "            </coding>",
      "            <text value=\"Delivery Point Identifier\"/>",
      "          </type>",
      "          <system value=\"http://hl7.org.au/id/dpid\"/>",
      "          <value value=\"${RANDOM}${RANDOM/\\d{4}$//}\"/>",
      "        </valueIdentifier>",
      "      </extension>",
      "      <type value=\"postal\"/>",
      "      <text value=\"TODO\"/>",
      "      <line value=\"TODO\"/>",
      "      <line value=\"TODO\"/>",
      "      <city value=\"TODO\"/>",
      "      <state value=\"${2|ACT,NSW,NT,QLD,SA,TAS,VIC,WA|}\"/>",
      "      <postalCode value=\"TODO\"/>",
      "      <country value=\"AU\"/>",
      "    </valueAddress>",
      "  </extension>",
      "  ${BLOCK_COMMENT_START} add PCA Location Identifier ${BLOCK_COMMENT_END}",
      "  <identifier/>",
      "  <status value=\"${3|active,inactive|}\"/>",
      "  <name value=\"TODO\"/>",
      "  <mode value=\"instance\"/>",
      "  <telecom/>",
      "  <address/>",
      "  ${BLOCK_COMMENT_START} Choose `physicalType` from preferred binding http://terminology.hl7.org.au/ValueSet/location-physical-type-extended ${BLOCK_COMMENT_END}",
      "  <physicalType>",
      "    <coding>",
      "      <system value=\"TODO\"/>",
      "      <code value=\"TODO\"/>",
      "      <display value=\"TODO\"/>",
      "    </coding>",
      "  </physicalType>",
      "  <managingOrganization>",
      "    <reference value=\"Organization/TODO\"/>",
      "    <display value=\"TODO\"/>",
      "  </managingOrganization>",
      "</Location>"
    ],
    "description": "This snippet inserts a full representation of a FHIR Location resource in XML, aligned to PCA Location of kind physical"
