      "  <operationalStatus>",
      "    <system value=\"http://terminology.hl7.org/CodeSystem/v2-0116\"/>",
      "    <code value=\"${2|C,H,I,K,O,U|}\"/>",
      "    <display value=\"TODO-display-value-for-${2}\"/>",
      "  </operationalStatus>",
      "  <name value=\"TODO\"/>",
      "  <alias value=\"TODO\"/>",
      "  <description value=\"TODO\"/>",
      "  <mode value=\"${3|instance,kind|}\"/>",
      "  ${BLOCK_COMMENT_START} Choose `type` from extensible binding http://terminology.hl7.org/ValueSet/v3-ServiceDeliveryLocationRoleType ${BLOCK_COMMENT_END}",
      "  <type>",
      "    <coding>",
      "      <system value=\"http://terminology.hl7.org/CodeSystem/v3-RoleCode\"/>",
      "      <code value=\"TODO\"/>",
      "      <display value=\"TODO\"/>",
      "    </coding>",
      "  </type>",
      "  <telecom/>",
      "  <telecom/>",
      "  <address/>",
      "  ${BLOCK_COMMENT_START} Choose `physicalType` from example binding http://hl7.org/fhir/ValueSet/location-physical-type ${BLOCK_COMMENT_END}",
      "  <physicalType>",
      "    <coding>",
      "      <system value=\"http://terminology.hl7.org/CodeSystem/location-physical-type\"/>",
      "      <code value=\"TODO\"/>",
      "      <display value=\"TODO\"/>",
      "    </coding>",
      "  </physicalType>",
      "  <position>",
      "    <longitude value=\"${RANDOM}.${RANDOM}\"/>",
      "    <latitude value=\"${RANDOM}.${RANDOM}\"/>",
      "    <altitude value=\"${RANDOM}.${RANDOM}\"/>",
      "  </position>",
      "  <managingOrganization>",
      "    <reference value=\"Organization/TODO\"/>",
      "    <display value=\"TODO\"/>",
      "  </managingOrganization>",
      "  <partOf>",
      "    <reference value=\"Location/TODO\"/>",
      "    <display value=\"TODO\"/>",
      "  </partOf>",
      "  <hoursOfOperation>",
      "    <daysOfWeek value=\"${4|mon,tue,wed,thu,fri,sat,sun|}\"/>",
      "    <allDay value=\"${5|true,false|}\"/>",
      "    <openingTime value=\"${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}\"/>",
      "    <closingTime value=\"${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}\"/>",
      "  </hoursOfOperation>",
      "  <availabilityExceptions value=\"TODO\"/>",
      "  <endpoint>",
      "    <reference value=\"Endpoint/TODO\"/>",
      "    <display value=\"TODO\"/>",
      "  </endpoint>",
      "</Location>"
