  "Example FHIR Location PCA mobile": {
    "isFileTemplate": true,
    "scope": "xml",
    "prefix": "Location-pca-mobile",
    "body": [
      "<?xml version=\"1.0\" encoding=\"utf-8\"?>",
      "${BLOCK_COMMENT_START} TODO customise to your scenario / use case ${BLOCK_COMMENT_END}",
      "<Location xmlns=\"http://hl7.org/fhir\">",
      "  <id value=\"${TM_FILENAME/(.*)\\..+$/$1/}\"/>",
      "  <meta>",
      "    <profile value=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-location\"/>",
      "  </meta>",
      "  ${BLOCK_COMMENT_START} optional extension PCA Amenity ${BLOCK_COMMENT_END}",
      "  <extension url=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-amenity\">",
      "    <valueCodeableConcept>",
      "      <coding>",
      "        <system value=\"http://pca.digitalhealth.gov.au/fhir/4.0/CodeSystem/pca-amenity\"/>",
      "        <code value=\"${1|wheelchairAccess,hearingInductionLoop,ttyFacilities,brailleSignage,publicTelephone,parkingFree,parkingPaid,babyChangeFacility,childMindingOrCreche,accessibleFriendlyToilet,toilet,cafeteria,accessibleParking,accessibleTelephones,lowSensorySpace|}\"/>",
      "        <display value=\"TODO-display-value-for-${1}\"/>",
      "      </coding>",
      "    </valueCodeableConcept>",
      "  </extension>",
      "  ${BLOCK_COMMENT_START} optional extension PCA Postal Address ${BLOCK_COMMENT_END}",
      "  <extension url=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-postal-address\">",
      "    <valueAddress>",
      "      <line value=\"TODO\"/>",
      "      <city value=\"TODO\"/>",
      "      <state value=\"${2|ACT,NSW,NT,QLD,SA,TAS,VIC,WA|}\"/>",
      "      <postalCode value=\"TODO\"/>",
      "      <country value=\"AU\"/>",
      "    </valueAddress>",
      "  </extension>",
      "  ${BLOCK_COMMENT_START} add PCA Location Identifier ${BLOCK_COMMENT_END}",
      "  <identifier/>",
      "  <status value=\"${3|active,inactive|}\"/>",
      "  <name value=\"TODO\"/>",
      "  <mode value=\"kind\"/>",
      "  <type>",
      "    <coding>",
      "      <system value=\"http://terminology.hl7.org/CodeSystem/v3-RoleCode\"/>",
      "      <code value=\"MOBL\"/>",
      "      <display value=\"Mobile Unit\"/>",
      "    </coding>",
      "  </type>",
      "  ${BLOCK_COMMENT_START} Choose at least one more `type` from required binding http://pca.digitalhealth.gov.au/fhir/4.0/ValueSet/pca-service-delivery-location-role-type ${BLOCK_COMMENT_END}",
      "  <type>",
      "    <coding>",
      "      <system value=\"http://terminology.hl7.org/CodeSystem/v3-RoleCode\"/>",
      "      <code value=\"${4|PTRES,SCHOOL,WORK,COMM,AMB|}\"/>",
      "      <display value=\"TODO-display-value-for-${4}\"/>",
      "    </coding>",
      "  </type>",
      "  <telecom/>",
      "  <address/>",
      "  ${BLOCK_COMMENT_START} Choose `physicalType` from preferred binding http://terminology.hl7.org.au/ValueSet/location-physical-type-extended ${BLOCK_COMMENT_END}",
      "  <physicalType>",
      "    <coding>",
      "      <system value=\"TODO\"/>",
      "      <code value=\"TODO\"/>",
      "      <display value=\"TODO\"/>",
      "    </coding>",
      "  </physicalType>",
      "  <managingOrganization>",
      "    <reference value=\"Organization/TODO\"/>",
      "    <display value=\"TODO\"/>",
      "  </managingOrganization>",
      "</Location>"
    ],
    "description": "This snippet inserts a full representation of a FHIR Location resource in XML, algned to PCA Location of kind mobile"
