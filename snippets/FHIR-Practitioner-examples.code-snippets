  "Example FHIR Practitioner PCA": {
    "isFileTemplate": true,
    "scope": "xml",
    "prefix": "Practitioner-pca",
    "body": [
      "<?xml version=\"1.0\" encoding=\"utf-8\"?>",
      "${BLOCK_COMMENT_START} TODO customise to your scenario / use case ${BLOCK_COMMENT_END}",
      "<Practitioner xmlns=\"http://hl7.org/fhir\">",
      "  <id value=\"${TM_FILENAME/(.*)\\..+$/$1/}\"/>",
      "  <meta>",
      "    <profile value=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-practitioner\"/>",
      "  </meta>",
      "  <extension url=\"http://pca.digitalhealth.gov.au/fhir/4.0/StructureDefinition/pca-active-in-system-of-record\">",
      "    <extension url=\"active\">",
      "      <valueBoolean value=\"${1|true,false|}\"/>",
      "    </extension>",
      "    <extension url=\"system-of-record\">",
      "      ${BLOCK_COMMENT_START} `system-of-record` has required binding https://pca.digitalhealth.gov.au/fhir/4.0/ValueSet-pca-system-of-record.html ${BLOCK_COMMENT_END}",
      "      <valueCodeableConcept>",
      "        <coding>",
      "          <system value=\"http://pca.digitalhealth.gov.au/fhir/4.0/CodeSystem/pca-system-of-record\"/>",
      "          <code value=\"${2|ABR,HI,Ahpra|}\"/>",
