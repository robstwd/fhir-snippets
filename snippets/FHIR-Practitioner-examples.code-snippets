      "  <qualification>",
      "    <identifier/>",
      "    ${BLOCK_COMMENT_START} qualification.code values from *example* binding http://terminology.hl7.org/ValueSet/v2-2.7-0360 ${BLOCK_COMMENT_END}",
      "    <code>",
      "      <coding>",
      "        <system value=\"http://hl7.org/fhir/v2/0360/2.7\"/>",
      "        <code value=\"${3|MD,DO,DC,DM,etc|}\"/>",
      "        <display value=\"TODO-display-value-for-${3}\"/>",
      "      </coding>",
      "    </code>",
      "    <period>",
      "      <start value=\"${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}\"/>",
      "      <end value=\"${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}\"/>",
      "    </period>",
      "    <issuer>",
      "      <reference value=\"Organization/TODO\"/>",
      "      <display value=\"TODO-display-value\"/>",
      "    </issuer>",
      "  </qualification>",
      "  <communication>",
      "    <coding>",
      "      <system value=\"urn:ietf:bcp:47\"/>",
